{
  "name": "react-template",
  "devDependencies": {
    "prettier": "^3.5.3",
    "syncpack": "14.0.0-alpha.12",
    "turbo": "^2.5.3",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint"
  }
}
