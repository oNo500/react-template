    "@kit101/eslint-config": "workspace:*",
    "@kit101/typescript-config": "workspace:*",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
